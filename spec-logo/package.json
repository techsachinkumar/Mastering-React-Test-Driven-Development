{
  "name": "spec-logo",
  "version": "1.0.0",
  "description": "A Logo interpreter with built-in specifications",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "build": "npm run build-server && webpack",
    "build-server": "babel server/src -d dist/server",
    "test-server": "jest --config server/test/jest.json",
    "serve": "node dist/server/server.js",
    "cucumber": "npx cucumber-js --require-module babel-register"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "cucumber": "^5.1.0",
    "express": "^4.16.4",
    "express-ws": "^4.0.0",
    "puppeteer": "^1.11.0",
    "redux": "^4.0.1",
    "redux-react-hook": "^3.0.1",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "expect-redux": "^4.0.1",
    "jest": "^23.6.0",
    "raw-loader": "^1.0.0",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "supertest": "^3.4.1",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "moduleNameMapper": {
      "\\.lgo$": "<rootDir>/test/lgoMock.js"
    }
  }
}
